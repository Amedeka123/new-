/* global style for variable width buttons */
a.var_btn_std, a.var_btn_std span {	background-image: url(/pages/art/var_standard_buttons/blue_button.gif); }
a.var_btn_blue, a.var_btn_blue span { background-image: url(/pages/art/var_standard_buttons/blue_button.gif); }
a.var_btn_black, a.var_btn_black span { background-image: url(/pages/art/var_standard_buttons/black_button.gif); }
a.var_btn_green, a.var_btn_green span { background-image: url(/pages/art/var_standard_buttons/green_button.gif); }
a.var_btn_red, a.var_btn_red span { background-image: url(/pages/art/var_standard_buttons/red_button.gif); }
a.var_btn_yellow, a.var_btn_yellow span { background-image: url(/pages/art/var_standard_buttons/yellow_button.gif); }
a.var_btn_green_arrow, a.var_btn_green_arrow span { background-image: url(/pages/art/var_standard_buttons/green_arrow_button.gif); }
a.var_btn_green_arrow_r, a.var_btn_green_arrow_r span { background-image: url(/pages/art/var_standard_buttons/green_arrow_button_right.gif); }
a.var_btn_blue_arrow, a.var_btn_green_arrow span { background-image: url(/pages/art/var_standard_buttons/blue_arrow_button.gif); }
a.var_btn_ghost, a.var_btn_ghost span { background-image: url(/pages/art/var_standard_buttons/ghost_button.gif); }
